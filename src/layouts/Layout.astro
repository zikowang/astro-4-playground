---
import Footer from "layouts/Footer.astro";
import Head from "./Head.astro";

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <Head title={title} />
    <body>
        <nav>
            <a href="/">Home</a>
        </nav>
        <slot />
        <Footer />
    </body>
</html>
